{
    "name": "geomtoy-examples",
    "version": "0.0.0",
    "description": "A 2D geometry responsive computing, visualizing and interacting library.",
    "author": "Fish Eleven",
    "homepage": "https://github.com/mog1347/geomtoy.git/#README",
    "repository": {
        "type": "git",
        "url": "https://github.com/mog1347/geomtoy.git"
    },
    "license": "MIT",
    "scripts": {
        "commit": "git-cz",
        "prepare": "husky install",
        "build-util": "cd geomtoy/packages/util && npm run bundle-js && npm run bundle-dts",
        "build-core": "cd geomtoy/packages/core && npm run bundle-js && npm run bundle-dts",
        "build-view": "cd geomtoy/packages/view && npm run bundle-js && npm run bundle-dts",
        "serve": "rollup --config --watch",
        "generate": "rollup --config --environment GENERATE"
    },
    "publishConfig": {
        "access": "public"
    },
    "typedocMain": "src/index.ts",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "browser": "dist/index.js",
    "types": "dist/index.d.ts",
    "devDependencies": {
        "@babel/core": "^7.16.7",
        "@babel/plugin-proposal-decorators": "^7.16.7",
        "@babel/preset-env": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@commitlint/cli": "^16.0.2",
        "@commitlint/config-conventional": "^16.0.0",
        "@commitlint/cz-commitlint": "^16.0.0",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-html": "^0.2.4",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.1.2",
        "@rollup/pluginutils": "^4.2.1",
        "commitizen": "^4.2.4",
        "dts-bundle-generator": "^7.2.0",
        "ejs": "^3.1.7",
        "husky": "^7.0.4",
        "prettier": "^2.5.1",
        "pretty-quick": "^3.1.3",
        "rollup": "^2.63.0",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-delete": "^2.0.0",
        "rollup-plugin-livereload": "^2.0.5",
        "rollup-plugin-serve": "^1.1.0",
        "rollup-plugin-terser": "^7.0.2",
        "symlink-dir": "^5.1.1",
        "typescript": "^4.5.4"
    }
}
